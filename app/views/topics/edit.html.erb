<h1>Edit the Topic Title</h1>
<p>Or don't. Whatever.</p>

<div class="col-md-8">
  <%= form_for @topic do |f| %>
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control', placeholder: "Enter topic title" %>
    </div>

      <%= f.submit "Save", class: 'btn btn-success' %>
  <%end%>
</div>
